import: cabal.project
import: cabal.project.freeze
profiling: True
library-profiling: True
executable-profiling: True
profiling-detail: late

program-options
  ghc-options: -fprof-auto -fprof-late -finfo-table-map -fdistinct-constructor-tables

package *
  ghc-options: -fprof-auto -fprof-late -finfo-table-map -fdistinct-constructor-tables
